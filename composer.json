{
    "name": "andreo/eventsauce-snapshotting",
    "description": "Extended snapshot components for EventSauce.",
    "keywords": [
        "eventsauce",
        "components",
        "snapshotting"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/dbal": "^3.5",
        "eventsauce/eventsauce": "^3.0",
        "eventsauce/uuid-encoding": "^0.3"
    },
    "require-dev": {
        "andreo/eventsauce-aggregate": "dev-main",
        "eventsauce/test-utilities": "^3.0",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Snapshotting\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
